{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": false,
        "color": {
            "defaultPalette": false,
            "palette": [
                { "slug": "u-gray-100", "color": "rgb(243 244 246)", "name": "Udemy Gray 100" },
                { "slug": "u-gray-200", "color": "rgb(229 231 235)", "name": "Udemy Gray 200" },
                { "slug": "u-gray-300", "color": "rgb(209 213 219)", "name": "Udemy Gray 300" },
                { "slug": "u-gray-400", "color": "rgb(156 163 175)", "name": "Udemy Gray 400" },
                { "slug": "u-gray-500", "color": "rgb(107 114 128)", "name": "Udemy Gray 500" },
                { "slug": "u-gray-600", "color": "rgb(75 85 99)", "name": "Udemy Gray 600" },
                { "slug": "u-gray-700", "color": "rgb(55 65 81)", "name": "Udemy Gray 700" },
                { "slug": "u-gray-800", "color": "rgb(31 41 55)", "name": "Udemy Gray 800" },
                { "slug": "u-gray-900", "color": "rgb(17 24 39)", "name": "Udemy Gray 900" },
                { "slug": "u-primary", "color": "rgb(239 68 68)", "name": "Udemy Primary" },
                { "slug": "u-accent", "color": "rgb(67 56 202)", "name": "Udemy Accent" },
                { "slug": "u-success", "color": "rgb(52 211 153)", "name": "Udemy Success" },
                { "slug": "u-info", "color": "rgb(96 165 250)", "name": "Udemy Info" },
                { "slug": "u-warning", "color": "rgb(251 146 60)", "name": "Udemy Warning" },
                { "slug": "u-danger", "color": "rgb(236 72 153)", "name": "Udemy Danger" }
            ],
            "link": true,
            "background": true,
            "defaultDuotone": false,
            "duotone": [
                { "colors": ["#ff0000", "#ffff00"], "slug": "red-and-yellow", "name": "Red and Yellow" },
                { "colors": ["#ff0000", "#00ff00"], "slug": "red-and-green", "name": "Red and Green" },
                { "colors": ["#ff0000", "#0000ff"], "slug": "red-and-blue", "name": "Red and Blue" },
                { "colors": ["#00ff00", "#ff0000"], "slug": "green-and-red", "name": "Green and Red" },
                { "colors": ["#00ff00", "#ffff00"], "slug": "green-and-yellow", "name": "Green and Yellow" },
                { "colors": ["#00ff00", "#0000ff"], "slug": "green-and-blue", "name": "Green and Blue" },
                { "colors": ["#0000ff", "#ff0000"], "slug": "blue-and-red", "name": "Blue and Red" },
                { "colors": ["#0000ff", "#00ff00"], "slug": "blue-and-green", "name": "Blue and Green" },
                { "colors": ["#0000ff", "#ffff00"], "slug": "blue-and-yellow", "name": "Blue and Yellow" },
                { "colors": ["#ffff00", "#ff0000"], "slug": "yellow-and-red", "name": "Yellow and Red" },
                { "colors": ["#ffff00", "#00ff00"], "slug": "yellow-and-green", "name": "Yellow and Green" },
                { "colors": ["#ffff00", "#0000ff"], "slug": "yellow-and-blue", "name": "Yellow and Blue" },
                { "colors": ["#ffffff", "#000000"], "slug": "white-and-black", "name": "White and Black" },
                { "colors": ["#000000", "#ffffff"], "slug": "black-and-white", "name": "Black and White" },
                { "colors": ["#000000", "#ff0000"], "slug": "black-and-red", "name": "Black and Red" }
            ],
            "defaultGradients": false,
            "gradients": [
                {"slug": "blue-to-green", "name": "Blue to Green", "colors": ["#4286f4", "#49f475"]},
                {"slug": "red-to-yellow-diagonal", "name": "Red to Yellow Diagonal", "colors": ["#ff0000", "#ffff00"]},
                {"slug": "purple-to-cyan", "name": "Purple to Cyan", "colors": ["#800080", "#00ffff"]},
                {"slug": "yellow-to-orange", "name": "Yellow to Orange", "colors": ["#ffff00", "#ff8000"]},
                {"slug": "green-to-blue-custom-shape", "name": "Green to Blue Custom Shape", "colors": ["#00ff00", "#0000ff"]},
                {"slug": "red-to-blue-conic", "name": "Red to Blue Conic", "colors": ["#ff0000", "#0000ff"]}
            ]
        },
        "typography": {
            "fontFamilies": [
                {"fontFamily": "Pacifico, sans-serif", "slug": "Pacifico","name": "Pacifico Font"}
            ]
        },
        "layout": {
           "contentSize": "800px",
           "wideSize": "1100px"
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "units": ["%", "px", "em", "vh", "vw"]
        },
        "blocks": {
            "core/site-title": {
                "color": {
                    "defaultPalette": true,
                    "custom": false
                },
                "typography": {
                    "fontFamilies": [
                        {"fontFamily": "Rubik, sans-serif", "slug": "Rubik","name": "Rubik Font"}
                    ]
                }
            }
        }        
    },
    "styles": {
        "color": {
            "text": "#ff0000",
            "background": "#dfe4ea"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "#000"
                }
            }
        },
        "blocks": {
            "core/button": {
                "color": {
                    "gradient": "var(--wp--preset--gradient--light-green-cyan-to-vivid-green-cyan)"
                }
            }
        },
        "spacing": {
            "margin": {
               "top":0,
               "bottom":0,
               "right":0,
               "left":0
            },
            "padding": {
                "top":0,
                "bottom":0,
                "right":0,
                "left":0
             }
        }
    }
}